alias gcc=/usr/bin/gcc-5
alias cpp=/usr/bin/cpp-5
alias cc=/usr/bin/gcc-5
alias ar=/usr/bin/gcc-ar-5
alias nm=/usr/bin/gcc-nm-5
alias gcc-ar=/usr/bin/gcc-ar-5
alias gcc-nm=/usr/bin/gcc-nm-5
